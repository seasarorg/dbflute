    override def equals(obj: Any) = {
        obj match {
            case obj: $myClassName => {
                val other: $myClassName = obj.asInstanceOf[$myClassName];
                {
#set ($equalsCount = 0)
#foreach ($col in $table.equalsHashcodeColumnList)
