#foreach ($columnName in $fk.LocalColumns)
#set ($localColumn = $table.getColumn($columnName))
#set ($foreignColumnName = $fk.getLocalForeignMapping().get($columnName))
            joinOnMap.put(getRealColumnName("${localColumn.name}"), ConditionQuery${fk.foreignPropertyNameInitCap}.getRealColumnName("${foreignColumnName}"));
#end
#if ($fk.hasFixedCondition())
            joinOnMap.put("$$fixedCondition$$", prepareFixedCondition("${fk.fixedCondition}", getRealAliasName(), ConditionQuery${fk.foreignPropertyNameInitCap}.getRealAliasName()));
#end
            getSqlClause().registerOuterJoin(ConditionQuery${fk.foreignPropertyNameInitCap}.getTableSqlName(), ConditionQuery${fk.foreignPropertyNameInitCap}.getRealAliasName(), joinOnMap);
        }
        protected String resolveNextRelationPath${fk.foreignPropertyNameInitCap}() {
            return resolveNextRelationPath("${table.name}", "${fk.foreignPropertyName}");
        }
