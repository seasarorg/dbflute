        protected ${foreignCQClassName} xcreateQuery${fk.ForeignPropertyNameInitCap}() {
            String nextRelationPath = resolveNextRelationPath${fk.foreignPropertyNameInitCap}();
            String resolvedAliasName = resolveJoinAliasName(nextRelationPath, getNextNestLevel());
            ${foreignCQClassName} cq = new ${foreignCQClassName}(this, getSqlClause(), resolvedAliasName, getNextNestLevel());
            cq.xsetForeignPropertyName("${fk.foreignPropertyName}");
            cq.xsetRelationPath(nextRelationPath);
            return cq;
        }
        public void xsetupOuterJoin_${fk.ForeignPropertyNameInitCap}() {
